#include <${PLUG_CONTROLLER_INCLUDE}>
#include <${PLUG_PROCESSOR_INCLUDE}>

namespace cycfi::qplug
{
   controller_ptr make_controller(base_controller& base)
   {
      return std::make_unique<${PLUG_CONTROLLER}>(base);
   }

   processor_ptr make_processor(base_processor& base)
   {
      return std::make_unique<${PLUG_PROCESSOR}>(base);
   }
}

